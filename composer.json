    "doctrine/orm": "^2.5"
  },
  "license": "proprietary",
  "author": [
    {
      "name": "Fabien Zanetti",
      "email": "fabienzanetti57@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
